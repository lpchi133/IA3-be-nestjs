{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "hooks": {
    "build": {
      "commands": [
        "npm run build",
        "npx prisma generate"
      ]
    }
  }
  }
  